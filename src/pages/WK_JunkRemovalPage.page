         function requestJunkRemovalJS() {             
             var serviceOrder = $('input[name=size]:checked').val();
             var redeempickup;
             if($(".redeemPickup input").is(':checked')){
                serviceOrder = "Full";
                redeempickup = $(".redeemPickup input").is(':checked');
             }
             var authContact = $('input[name=survey]:checked').val();
             var preference = $('input[name=pickup-time]:checked').val();
             var dateFormat = $('#future-date-input').val();
             var pickupMode = $('input[name=pickup-mode]:checked').val();
             var costEstimate = document.getElementById("estimateCost").innerHTML;
             var latOther = document.getElementById("estimateCostLat").innerHTML;
             var longOther = document.getElementById("estimateCostLong").innerHTML;
             requestJunkRemovalAF(serviceOrder,authContact,preference,dateFormat,pickupMode,costEstimate,latOther,longOther,redeempickup);
         }
         </script>
         <script>
         function getAddressDetails() {
             var street = document.getElementById("{!$Component.formId.Address}").value;
              var city = document.getElementById("{!$Component.formId.City}").value;
             var state = document.getElementById("{!$Component.formId.State}").value;
             var zipCode = document.getElementById("{!$Component.formId.ZipCode}").value;
             var country = 'USA';
             //alert(street+city+state+zipCode+country);
             return (
                  street +','+
                  city +','+
                  state +','+ 
                  zipCode +','+
                  country
                 );
         }
         
         function getLatLng() 
        { 
         var serviceOrder = $('input[name=size]:checked').val();
         var costValues = '{!costValues}';
         if($(".redeemPickup input").is(':checked')){
            serviceOrder = "Full";
         }

        //alert(serviceOrder);
         geocoder = new google.maps.Geocoder();  
         geocoder.geocode( 
         { 'address': getAddressDetails() }, 
         function(results, status) 
         { 
          if (status == google.maps.GeocoderStatus.OK) 
          {// alert('Hi');
              var lat = (results[0].geometry.location.lat().toString());
              var longi = (results[0].geometry.location.lng().toString());
              document.getElementById("estimateCostLat").innerHTML = lat;
              document.getElementById("estimateCostLong").innerHTML = longi;
              
              WK_JunkRemovalController.calculateFormula(lat,longi,serviceOrder,costValues,
                        function(result, event) {
                           // alert('het');
                            if (event.type === 'exception') {
                               alert('he'+event.message);
                                console.log(event);
                            }
                            else if (event.status) {
                                if (result != null) {
                                    result = Math.round(result );
                                    document.getElementById("estimateCost").innerHTML = '$' + result;
                                    $('.requestBtn').removeClass("disableContnue");
                                   // alert(result);
                                   if($(".redeemPickup input").is(':checked')){
                                    // Redeemed Coupon
                                    document.getElementById("estimateCost").innerHTML = '$0' ;
                                    $('.requestBtn').removeClass("disableContnue");
                                   }



                                }
                            }
                        }
                    );
          } 
          else if (status == google.maps.GeocoderStatus.ZERO_RESULTS || status == google.maps.GeocoderStatus.INVALID_REQUEST) 
          { 
          // alert('Problem with Address'); 
          } 
          else if (status == google.maps.GeocoderStatus.OVER_QUERY_LIMIT) 
          { 
          // alert('Google Exhausted'); 
          } 
         });
          
        }
         </script>
         
